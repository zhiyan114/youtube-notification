{
    "compilerOptions": {
        "outDir": "./dist",
        "allowJs": true,
        "module": "commonjs",
        "target": "es2015",
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
    },
        "include": [
        "./src/**/*",
    ]
}